{
  "name": "@ea-utilities/db-sync",
  "version": "1.0.12",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "run:server": "ts-node src/server/main.ts",
    "start:server": "nodemon --config src/server/nodemon.json",
    "start": "node public/server/main.js",
    "start:watch": "nodemon --config src/server/nodemon.json",
    "build": "tsc --project src/server/tsconfig.json",
    "start:client": "webpack serve --config src/client/webpack.config.js",
    "build:client": "webpack --config src/client/webpack.config.js",
    "build:all": "npm run clean && tsc",
    "npm:publish": "npm run build:all && npm publish ."
  },
  "author": "Edwin Arias",
  "license": "ISC",
  "description": "",
  "keywords": [
    "indexedDB",
    "telegram",
    "gramjs",
    "database",
    "offline database",
    "db",
    "react",
    "angular",
    "vue",
    "express",
    "web sockets"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-ws": "^5.0.2",
    "morgan": "^1.10.0",
    "telegram": "^2.25.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.4",
    "html-webpack-plugin": "^5.6.0",
    "node": "^22.8.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}